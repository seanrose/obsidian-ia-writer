{
	"name": "obsidian-ia-writer",
	"version": "0.1.0",
	"description": "Adds a command to open the currently active note in iA Writer.",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json"
	},
	"keywords": [
		"obsidian",
		"ia-writer",
		"markdown",
		"writing"
	],
	"author": "Sean Rose",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/seanrose/obsidian-ia-writer"
	},
	"devDependencies": {
		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"obsidian": "latest",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	}
}
